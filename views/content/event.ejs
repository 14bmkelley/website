<span class="main">
  <div class="container">
    <div class="main_section">
      <div id="event">
        <div>
          <img width=200px height=200px align=left src=<%= event["picture"] %> />
          <h1><%= event["name"] %></h1>
          <h2><%= event["time"] %> on <%= event["date"] %> at <%= event["location"] %></h2>
          <p><%= event["description"] %></p>
        </div>
        <div>
          <% if (event["cost"] !== "0") { %>
            <p>Cost: <%= event["cost"] %></p>
          <% } %>
          <p>List of Attendees:</p>
          <ul>
            <% for (var i = 0; i < event["attending"].length; i++) { %>
              <li><%= event["attending"][i] %></li>
            <% } %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</span>
